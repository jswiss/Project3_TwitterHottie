<body id="profile-bg">
  <header>
      <p id="search-link"><a id="map-link" href="/map">Find tweets</a></p>
      <div id="profile-pic-header"><a href="/profile"><img src="<%= user.twitter.profileImage %>"></div>
      <div id="username-logged-in"><a id="username-link" href="/profile"><%= user.twitter.username %></a></div>
  </header>

  <div class="container">

      <div class="row">

          <!-- TWITTER INFORMATION -->
          <div class="col-sm-3">
            <div class="well">
                  <p id="profile-text">
                  <a target="_blank" href="http://www.twitter.com/<%= user.twitter.username %>"><img id="profile-pic" src="<%= user.twitter.profileImage %>"></a>
                    <br>
                    <strong><a class="profile-twit-link" target="_blank" href="http://www.twitter.com/<%= user.twitter.username %>"> @<%= user.twitter.username %> </a></strong>
                    <br>
                  </p>
                  <a href="/map" class="btn btn-default btn-sm">Map</a>
                  <a href="/logout" class="btn btn-default btn-sm">Logout</a>

            </div>
          </div>
          <div class="col-sm-9">
            <% for(var i=0; i < photos.length; i++) { %>
              <div class="card card-block">
              
              <div class="card-title"> <a class="profile-twit-link" target="_blank" href="http://www.twitter.com/<%= photos[i].screenName %>"> @<%= photos[i].screenName %> </a></div> 
              <img class="card-img" src="<%= photos[i].url%>">

              </div>
            <% } %>
          </div>

      </div>

  </div>
</body>